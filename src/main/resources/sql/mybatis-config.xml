<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
	PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-config.dtd"
>

<configuration>
  <settings>
    <setting name="logImpl" value="LOG4J" />
    <setting name="cacheEnabled" value="true" />
    <setting name="lazyLoadingEnabled" value="true" />
    <setting name="multipleResultSetsEnabled" value="true" />
    <setting name="useColumnLabel" value="true" />
    <setting name="useGeneratedKeys" value="false" />
    <setting name="defaultExecutorType" value="SIMPLE" />
    <setting name="defaultStatementTimeout" value="100" />
    <setting name="mapUnderscoreToCamelCase" value="true" />
  </settings>


  <typeAliases>
    <typeAlias alias="UserDto" type="com.inno.odag.dto.UserDto" />
    <typeAlias alias="CategoryDto" type="com.inno.odag.dto.CategoryDto"/>
    <typeAlias alias="TokenDto" type="com.inno.odag.dto.TokenDto"/>
    <typeAlias alias="OpenapiDto" type="com.inno.odag.dto.OpenapiDto"/>
    <typeAlias alias="BookmarkDto" type="com.inno.odag.dto.BookmarkDto"/>
    <typeAlias alias="BookmarkOpenapiDto" type="com.inno.odag.dto.BookmarkOpenapiDto"/>
    <typeAlias alias="OpenidDto" type="com.inno.odag.dto.OpenidDto"/>
    <typeAlias alias="RecommendOpenapiDto" type="com.inno.odag.dto.RecommendOpenapiDto"/>
    <typeAlias alias="UserOpenapiDto" type="com.inno.odag.dto.UserOpenapiDto"/>
    <typeAlias alias="SearchDto" type="com.inno.odag.dto.SearchDto"/>
    <typeAlias alias="NoticeDto" type="com.inno.odag.dto.NoticeDto"/>
    <typeAlias alias="CommunityDto" type="com.inno.odag.dto.CommunityDto"/>
    <typeAlias alias="CommunityReplyDto" type="com.inno.odag.dto.CommunityReplyDto"/>
  </typeAliases>
  
  
  <plugins>
    <plugin interceptor="com.inno.odag.interceptor.MyBatisSqlLogIntercetor"/>
  </plugins>
  
</configuration>